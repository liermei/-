#include<iostream>
#include<vector>
#include<string>
#include<algorithm>
#include<functional>
using namespace std;
using namespace std::placeholders;
bool check_size(const string&s, int n)
{
	return n > s.size();
}
int main()
{
	vector<int>vint;
	for (int i = 0; i != 99; i++)
		vint.push_back(i);
	auto iter = find_if(vint.begin(), vint.end(), bind(check_size, "accumulate", _1));
	cout << *iter << endl;
	system("pause");
	return 0;
}
