#include<iostream>
#include<vector>
#include<string>
#include<algorithm>
using namespace std;
void elimDups(vector<string>&words)
{
	sort(words.begin(), words.end());
	auto end_words = unique(words.begin(), words.end());
	words.erase(end_words, words.end());
	for (auto c : words)
		cout << c << " ";
}
int main()
{	
	vector<string>str = { "i","have","a","dream","and","i","have","to","finish","it" };
	elimDups(str);
	system("pause");
	return 0;
}
