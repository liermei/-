#include<iostream>
#include<list>
#include<iterator>
#include<vector>
using namespace std;
int main()
{
	vector<int>vint = { 1,2,3,4,5,6,7,8,9,0 };
	list<int>lint;
	auto viter = vint.crbegin()+3;
	while (viter != vint.crbegin() + 8)
		lint.push_back(*viter++);
	for (auto p : lint)
		cout << p << " ";
	cout << endl;
	system("pause");
	return 0;
}

