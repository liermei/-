#include<iostream>
#include<vector>
#include<string>
#include<algorithm>
using namespace std;
void something(int a)
{
	auto f=[a]()mutable->bool
	{if (a > 0) {
		--a;
		return false;
	}
	else
		return true;
	};
	for (int j = 0; j < 6; j++)
		cout << f() << " ";
	cout << endl;
}
int main()
{	
	something(5);
	system("pause");
	return 0;
}
